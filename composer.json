{
    "name": "10quality/wpmvc-addon-metaboxer",
    "description": "Metabox generator for WordPress MVC (WPMVC) framework models.",
    "license": "MIT",
    "keywords": ["wpmvc","wordpress","metaboxer"],
    "require": {
        "php": ">=5.4.0",
        "10quality/wpmvc-core": ">=3.1.0",
        "10quality/wpmvc-addon": "1.0.*",
        "10quality/wpmvc-addon-resources": "1.0.*"
    },
    "autoload": {
        "files": [
            "addon/Lib/functions.php"
        ],
        "psr-4": {
            "WPMVC\\Addons\\Metaboxer\\": "addon/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "10quality/wpmvc-addon-testsuite": "dev-main",
        "phpunit/phpunit": "9.*"
    }
}
